<% const bunuses = player.getBonuses(['coins', 'successChance', 'staminaCost']);%>

<%if (bunuses && Object.keys(bunuses).length){%>
<h6 class="label-sub-title"> <%=word.BONUSES%></h6>
<ul class="list-group list-group-flush">
  <%if (bunuses['coins']){%>
  <%- include('../../single/section-line', { text: word.COINS_FIND, value: bunuses['coins'].sign().percent() }); %>
  <%}%>

  <%if (bunuses['successChance']){%>
  <%- include('../../single/section-line', { text: word.SUCCESS_CHANCE, value: bunuses['successChance'].sign().percent() }); %>
  <%}%>

<%if (bunuses['staminaCost']){%>
  <%- include('../../single/section-line', { text: word.STAMINA_COST, value: bunuses['staminaCost'].sign().percent() }); %>
  <%}%>
</ul>
  <%}%>