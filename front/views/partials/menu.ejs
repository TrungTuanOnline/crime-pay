<ul class="nav nav-tabs">

  <%
  let menuItems = [word.ROBBERY, word.PRISON, word.CLUB, word.MARKET, word.INVENTORY, word.HISTORIC, word.SETTINGS ];

  
  if (locals?.player?.lifeImprisonment) menuItems = [word.PRISON , word.INVENTORY, word.HISTORIC, word.SETTINGS];
%>

  <%menuItems.forEach((each)=>{ %>

  <li class="nav-item">
    <a class="nav-link <%=locals['nav_'+ each.toLowerCase()] ? 'active' : ''%>" aria-current="page" href="/<%=each.toLowerCase()%>" onclick="return location.href != this.href">
      <%=each%>
    </a>
  </li>

  <%})%>


</ul>