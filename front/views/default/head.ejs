<meta charset="UTF-8">
<!DOCTYPE html>

<head>
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Press+Start+2P">

  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js">
    < script src = "https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
    integrity = "sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
    crossorigin = "anonymous" >
  </script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <link rel="stylesheet" href="/css/default.css">

  <script src="/js/lib/toast.js" crossorigin="anonymous"></script>
  <script src="https://momentjs.com/downloads/moment.min.js" crossorigin="anonymous"></script>

  <script type="text/javascript" src="/lib/util.js"></script>


  <title><%=word.APP_NAME + (locals?.pageTitle ? ' - '.join(locals?.pageTitle): '')%></title>


  <% 
  
  if (locals?.carousel) locals.css = ['lib/cards-carousel', ...(locals?.css || [])];

  if (locals?.carousel) locals.js = ['lib/cards-carousel','lib/form-control', 'lib/keep', ...(locals?.js || [])];
  
  if (locals?.buttons || locals?.carousel) locals.js = ['lib/request-button','lib/buttons-bar', ...(locals?.js || [])];

  if (locals?.player) locals.css = ['player-status', ...(locals?.css || [])];

  if (locals?.player) locals.js = ['player-status', 'mutation/player', ...(locals?.js || [])];
  %>


  <% locals?.css?.array().forEach((each) => { %>
  <link rel="stylesheet" href="/css/<%=each%>.css">
  <%})%>

  <% locals?.js?.array().forEach((each) => { %>
  <script src="/js/<%=each%>.js"></script>
  <%})%>


</head>